  {
    "key": "alt+c",
    "command": "vim.remap",
    "when": "editorTextFocus && vim.active",
    "args": {
      "before": ["<A-c>"],
      "after": ["<Esc>"]
    }
  },
  {
    "key": "q",
    "command": "vim.remap",
    "when": "editorTextFocus && vim.mode == 'Normal'",
    "args": {
      "before": ["q"],
      "after": ["i"]
    }
  },
  {
    "key": "Q",
    "command": "vim.remap",
    "when": "editorTextFocus && vim.mode == 'Normal'",
    "args": {
      "before": ["Q"],
      "after": ["I"]
    }
  },
  {
    "key": "=",
    "command": "vim.remap",
    "when": "editorTextFocus && (vim.mode == 'Normal' || vim.mode == 'Insert' || vim.mode == 'Visual')",
    "args": {
      "before": ["="],
      "after": ["+"]
    }
  },
  {
    "key": "w",
    "command": "vim.remap",
    "when": "editorTextFocus && (vim.mode == 'Normal' || vim.mode == 'Insert' || vim.mode == 'Visual')",
    "args": {
      "before": ["w"],
      "after": ["b"]
    }
  },
  {
    "key": "alt+c",
    "command": "vim.remap",
    "when": "editorTextFocus && (vim.mode == 'Normal' || vim.mode == 'Insert' || vim.mode == 'Visual')",
    "args": {
      "before": ["<A-c>"],
      "after": ["<Esc>"]
    }
  },
